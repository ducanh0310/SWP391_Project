<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>ViewProfilePatientController</servlet-name>
        <servlet-class>controller.patient.ViewProfilePatientController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditProfilePatientController</servlet-name>
        <servlet-class>controller.patient.EditProfilePatientController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadImageProfileController</servlet-name>
        <servlet-class>controller.imageProfile.UploadImageProfileController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadImage</servlet-name>
        <servlet-class>controller.imageProfile.UploadImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadImageController</servlet-name>
        <servlet-class>controller.imageProfile.UploadImageController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadController</servlet-name>
        <servlet-class>controller.imageProfile.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadcareUploader</servlet-name>
        <servlet-class>controller.imageProfile.UploadcareUploader</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet</servlet-name>
        <servlet-class>controller.imageProfile.UploadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewProfilePatientController</servlet-name>
        <url-pattern>/patient/profile/view</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditProfilePatientController</servlet-name>
        <url-pattern>/patient/profile/edit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadImageProfileController</servlet-name>
        <url-pattern>/uploadImage</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadImage</servlet-name>
        <url-pattern>/uploadimg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadImageController</servlet-name>
        <url-pattern>/UploadImageController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadController</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadcareUploader</servlet-name>
        <url-pattern>/UploadcareUploader</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet</servlet-name>
        <url-pattern>/uploads</url-pattern>
    </servlet-mapping>
</web-app>
